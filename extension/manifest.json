{
	"name": "WaspLine Reader",
	"version": "1.0",
	"description": "Read faster using color gradients",
	"manifest_version": 2,
	"background":{
		"scripts":["bg.js"]
        },
	"browser_action": {
		"default_icon": {
			"16": "icons/wasp16.png",
			"24": "icons/wasp24.png",
			"32": "icons/wasp32.png"
		},
		"default_title": "Apply gradients"
	},
	"icons": {
		"16": "icons/wasp16.png",
		"48": "icons/wasp48.png",
		"128": "icons/wasp128.png"
	},
	"permissions": ["activeTab"]

}
